}

.site-header__logo {
  //remove default margin when this is an <h1> tag
  margin: 0;
  min-height: 80px;

  img {
    max-height: $height-site-header;
    display: block;
    margin: 0 auto;
  }

  .site-header__logo-link {
    color: $color-navigation-text;
    display: block;
    text-align: center;

    &:hover,
    &:focus,
    &:active {
      opacity: 1;
    }
  }

  @include media-query($small) {
    font-size: em(18px);

    img {
      max-width: 100%;
    }
  }
}


.site-header__cart .icon,
.site-header__search .icon {
  font-size: em(25);
  padding: $gutter-site / 4;

  @include media-query($medium-up) {
    padding: $gutter-site / 2;
  }
}

.site-header__search .icon {
  padding-right: 0;
}

.cart-link {
  position: relative;

  &.cart-bubble--visible {
    &:before {
      content: '';
      position: absolute;
      top: 18px;
      right: 4px;
      width: 10px;
      height: 10px;
      background-color: $color-sale-badge;
      border-radius: 50%;

      @include media-query($small) {
        top: 14px;
        right: 0;
      }
    }
  }
}

.action-area {
  position: relative;
  overflow: hidden;
}

.action-area {
  z-index: $z-index-action-bar;

  .js-sticky-action-bar & {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    @include transform(translate3d(0, -100%, 0));
  }

  .js-sticky-action-bar--open & {
    @include transform(translate3d(0, 0, 0));
    transition: all 0.35s cubic-bezier(0.33, 0.59, 0.14, 1);
  }
}

.action-bar-wrapper {
  &.scrollable-js {
    // only show gradients if pointer-events are supported
    .supports-pointerevents & {
      &:before,
      &:after {
        content: '';
        pointer-events: none;
        z-index: $z-index-search-bar;
        width: 125px;
        height: $height-action-bar + $height-action-bar-active-indicator;
        color: $color-action-bar-text;
        position: absolute;
        top: 0;
        bottom: 0;
        @include transform(translate3d(0, 0, 0));
      }

      &:before {
        @include gradient(right, $color-action-bar-bg, $color-action-bar-bg);
        right: 0;
      }

      &:after {
        @include gradient(left, $color-action-bar-bg, $color-action-bar-bg);
        left: 0;
        opacity: 0;
      }

      &.scrolled {
        &:after {
          opacity: 1;
        }
      }
    }
  }
}

.action-bar {
  position: relative;
  overflow: hidden;
  background: $color-action-bar-bg;
  height: $height-action-bar + $height-action-bar-active-indicator;
  @include transform(translateZ(0)); // for re-paint performance

  .scrollable-js & {
    overflow-x: auto;
    overflow-y: hidden;
    -webkit-overflow-scrolling: touch;
  }
}

.action-bar__menu--main,
.action-bar__menu--sub {
  position: absolute;
  height: $height-action-bar;
  min-width: 100%;
  margin-top: 0;
  margin-bottom: $height-action-bar-active-indicator;
  text-align: center;
  white-space: nowrap;
  background-color: $color-action-bar-bg;
  transition: all 200ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  display: none;
  top: -$height-action-bar;

  &.action-bar--show {
    top: 0;
    visibility: visible;
    display: block;

    .scrollable-js & {
      padding-right: 100px;
    }
  }
}

.action-bar--active {
  position: relative;

  &:after {
    content: '';
    display: block;
    position: absolute;
    height: $height-action-bar-active-indicator;
    left: 10px;
    right: 10px;
    background: $color-action-bar-active;
  }

  @include media-query($medium-up) {
    &:after {
      left: 15px;
      right: 15px;
    }
  }
}

.action-bar__link {
  @include header-font-stack();
  font-size: $font-size-button; // 15px
  position: relative;
  display: inline-block;
  color: $color-action-bar-text;
  padding: 0 10px;
  background-color: $color-action-bar-bg;
  border: 0 none;
  line-height: $height-action-bar;

  @include media-query($small) {
    font-size: $font-size-button-mobile;
  }

  &:hover,
  &:active,
  &:focus {
    opacity: 1;
  }

  .action-bar--active & {
    color: $color-action-bar-active;

    &:hover,
    &:active,
    &:focus {
      opacity: 1;
    }
  }
}

.action-bar__back {
  border: 0px solid transparent;
  background-color: $color-action-bar-bg;

  &:focus,
  &:active {
    background-color: $color-action-bar-bg;
  }
}

@include media-query($medium-up) {
  .action-bar__link {
    padding-left: 15px;
    padding-right: 15px;
  }
}

/*================ Secondary action bar ================*/
.action-bar-secondary {
  @include clearfix;
  position: relative;
  margin: $gutter-site;
}

@include media-query($small) {
  .action-bar-secondary__dropdown {
    text-align: center;

    &:first-of-type {
      margin-bottom: 20px;
    }
  }
}

@include media-query($medium-up) {
  .action-bar-secondary--left {
    float: left;
  }

  .action-bar-secondary--right {
    float: right;
  }
}

.search-bar {
  color: $color-drawers-text;
  background-color: $color-drawers;
  border-bottom: 1px solid rgba(0,0,0,0.1);
  height: $height-site-header;  
  padding: 0 ($gutter-site / 2);
}

.search-bar__table {
  display: table;
  table-layout: fixed;
  width: 100%;
  height: $height-site-header;
}

.search-bar__table-cell {
  display: table-cell;
  vertical-align: middle;
}

.supports-fontface .search-bar__icon-cell {
  width: 40px;
}

.search-bar__form {
  width: 90%;
}

.search-bar__input {
  @include placeholder-text($color-drawers-text, 0.8);
  background: none;
  border: none;
  color: $color-drawers-text;
  width: 100%;
  padding: 0 10px;
  height: $height-site-header - 10px;
}

.search-bar__icon-button {
  background: none;
  border: none;
  display: inline-block;
  color: $color-drawers-text;
  padding: 0 10px;
  height: $height-site-header - 10px;

  .icon {
    font-size: em(20px);
  }
}

.search__results-count {
  color: $color-action-bar-text;
  padding: 0 10px 10px 20px;
  line-height: $height-action-bar;
}

@include media-query($medium-down) {
  .product-item {
    margin-bottom: $gutter-site * 2;
  }
}

.product-item__link-wrapper {
  @include display-flexbox();
  width: 100%;
  position: relative;

  .no-js & {
    text-align: center;
  }
}

.product-item__link {
  position: relative;
  width: 100%;
  height: 100%;
  min-height: 120px;
  overflow: hidden;
  -webkit-flex: 1;
  -moz-flex: 1;
  -ms-flex: 1;
  flex: 1;
  -ms-flex-item-align: center;
  -webkit-align-self: center;
  align-self: center;
  color: $color-body-text;

  &:hover,
  &:focus {
    opacity: 1;
  }

  p {
    margin: 0 15px 2px 15px;

    &:last-child {
      margin-bottom: 0;
    }
  }

  .product-item__image-wrapper {
    position: relative;
    height: 0;
    display: block;
    width: 100%;
  }
}

.product-item__image-container {
  margin: 0 auto;

  .no-js & {
    @extend .visually-hidden;
  }
}

.product-item__image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  margin: 0 auto $gutter-site;
  transition: opacity 0.2s ease;

  @include media-query($large-up) {
    margin-bottom: 0;

    .product-item__link:hover & {
      opacity: 0.1;
    }
  }
}

.product-item__image-no-js {
  margin: 0 auto;
}

.product-item__image--margins {
  margin: 30px;

  @include media-query($small) {
    margin-bottom: 0;
  }
}

.product-item__title {
  @include header-font-stack();
  color: $color-headings-text;
  font-size: em(floor($font-size-header * 0.83)); // 20px

  @include media-query($medium-down) {
    font-size: em(floor($font-size-header * 0.6)); // 15px
  }
}

.product-item__meta__inner {
  text-align: center;

  @include media-query($medium-down) {
    text-align: left;
    padding-top: $gutter-site;
    float: left;
    width: 100%;
  }

  // left align text on mobile when product 2 products are shown per row
  @include media-query($medium-down) {
    .product-item.small--one-half & {
      text-align: left;
    }
  }
}

@include media-query($large-up) {
  .product-item__meta {
    display: block;
    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    opacity: 0;
    filter: alpha(opacity=0);
    transition: opacity 0.4s ease;

    &:before {
      content: '';
      position: absolute;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      opacity: 0;
      background-color: $color-body;
      transition: opacity 0.4s ease;
    }

    &:hover,
    &:focus {
      opacity: 1;
      filter: alpha(opacity=100);

      .product-item__meta__inner {
        top: 50%;
      }

      &:before {
        opacity: 0.4;
      }
    }
  }

  .product-item__meta__inner {
    position: absolute;
    top: 52%;
    left: 0;
    right: 0;
    padding: 0 $gutter-site;
    transition: top 0.3s ease;
    @include transform(translateY(-50%));
  }
}

.product-item__vendor,
.product-item__price-wrapper {
  font-size: $font-size-base;

  @include media-query($medium-down) {
    font-size: $font-size-base - 2px;
  }
}

/*================ Price styles ================*/
.product-item__price--reg {
  opacity: 0.7;
}

.product-item__price--sale {
  color: $color-sale-badge;
  padding-left: 5px;
}

/*================ Availability badges ================*/
.product-item__badge {
  position: absolute;
  top: -26px;
  right: -94px;
  width: 200px;
  padding: 40px 4px 4px;
  text-align: center;
  background-color: $color-sale-badge;
  color: $color-sale-badge-text;
  font-size: em(13);
  font-weight: $font-weight-bold;
  @include transform(rotate(45deg) translateZ(0));

  .ie9 & {
    padding-top: 4px;
    top: 0;
    right: 0;
  }
}

.product-item__badge--large {
  top: -10px;
  right: -75px;
  width: 200px;
}

/*================ Articles ================*/
.article-meta {
  margin-bottom: $gutter-site;

  li + li {
    position: relative;
    padding-left: 18px;
    margin-left: 10px;

    &:before {
      content: '\2022';
      display: block;
      position: absolute;
      left: 0;
    }
  }
}

/*================ Comments ================*/
.comment-list {
  margin: 0 0 $gutter-site;
  padding: 0;
}

.comment {
  list-style: none;
  padding: 0;
  margin: 0 0 ($gutter-site * 2);
}

.comment__content {
  p:last-child {
    margin-bottom: $gutter-site / 2;
  }
}

.comment__textarea {
  min-height: 150px;
}

// Comment success message styles
@include media-query($small) {
  .comments-wrapper {
    :target {
      padding-top: $height-action-bar;
    }
  }
}

/*================ Rich text section ================*/
.rich-text__text--large {
  font-size: em(floor($font-size-base * 1.15));
}
.rich-text__text--small {
  font-size: em(floor($font-size-base * 0.9));
}

/*================ Indented article/page images and blockquotes ================*/

@include media-query($medium-up) {
  .rte--indented-images {
    img:not([style]),
    img[style="float: none;"] {
      max-width: 140%;
      margin-left: -20%;

      // Class added if image isn't wider than parent
      &.rte__no-indent {
        margin: 0 auto;
        max-width: 100%;
      }
    }

    img[style="float: right;"] {
      margin: 0 -10% ($gutter-site / 2) ($gutter-site / 2);
    }

    img[style="float: left;"] {
      margin: 0 ($gutter-site / 2) ($gutter-site / 2) -10%;
    }
  }
}

@include media-query($small) {
  .rte--indented-images {
    img:not([style]),
    img[style="float: none;"] {
      max-width: 120%;
      margin-left: -10%;
    }
  }
}

/*================ RTE collection description ================*/
.rte--description {
  text-align: center;
  font-size: em(17px);
  padding: ($gutter-site * 3) ($gutter-site * 2) ($gutter-site * 2);

  ol,
  ul {
    list-style-position: inside;
    margin-left: 0;
  }
}

/*================ General ================*/
.rte {
  table {
    margin-bottom: $gutter-site;
  }

  th,
  td {
    border: 1px solid $color-border;
    padding: ($gutter-site / 2) ($gutter-site / 0.75);
  }

  &.text-center,
  .text-center & {
    ul,
    ol {
      margin-left: 0;
      list-style-position: inside;
    }
  }
}

.social-sharing {
  margin-left: -($gutter-site / 2);
  margin-top: $gutter-site;
  margin-bottom: $gutter-site;
}

.social-icons--drawer {
  margin-top: $gutter-site * 1.5;

  @include media-query($medium-down) {
    margin-left: $gutter-site / 2;
  }
}

.social-sharing a {
  display: inline-block;
  border: 1px solid $color-border;
  color: $color-body-text;
  padding: 10px 15px;
  margin-left: ($gutter-site / 2);
  margin-bottom: ($gutter-site / 2);

  .icon {
    margin-right: 5px;
  }
}

.icon {
  .share-facebook & {
    color: #3b5998;
  }

  .share-facebook & {
    color: #3b5998;
  }

  .share-twitter & {
    color: #00aced;
  }

  .share-pinterest & {
    color: #cb2027;
  }
}

/*============================================================================
  Drawer skeleton
==============================================================================*/
.is-transitioning {
  display: block !important;
  visibility: visible !important;
}

.js-drawer-open {
  overflow: hidden;
}

.drawer {
  display: none;
  position: fixed;
  overflow-y: auto;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
  bottom: 0;
  z-index: $z-index-drawer;
  color: $color-drawers-text;
  background-color: $color-drawers;
  transition: $drawer-transition;
}

.drawer--left,
.drawer--right {
  top: 0;
  max-width: 95%;
}

.drawer--left {
  width: $drawer-nav-width;
  left: -$drawer-nav-width;

  .js-drawer-open-left & {
    display: block;
    @include transform(translateX($drawer-nav-width));
  }
}

.drawer--right {
  width: $drawer-cart-width;
  right: -$drawer-cart-width;

  .js-drawer-open-right & {
    display: block;
    @include transform(translateX(-$drawer-cart-width));
  }

  .drawer__close {
    float: right;
    height: $drawer-header-height;
  }
}

.drawer--top {
  width: 100%;
  height: $height-site-header;
  top: -$height-site-header;

  .js-drawer-open-top & {
    display: block;
    @include transform(translateY(100%));
  }
}

#PageContainer {
  overflow: hidden;
}

.drawer-page-content:after {
  visibility: hidden;
  opacity: 0;
  content: '';
  display: block;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.6);
  z-index: $z-index-drawer - 1;
  transition: $drawer-transition;

  .js-drawer-open & {
    visibility: visible;
    opacity: 1;
  }
}

.drawer__title,
.drawer__close {
  display: table-cell;
  vertical-align: middle;
}

.drawer__title {
  @include header-font-stack();
  font-size: em(floor($font-size-header * 0.71)); //17px
  width: 100%;
}

.drawer__close-button {
  background: none;
  border: 0 none;
  position: relative;
  right: -15px;
  height: 100%;
  width: 60px;
  padding: 0 20px;
  color: inherit;
  font-size: em(18);

  .drawer--left & {
    right: auto;
    left: -15px;
  }

  .drawer--product & {
    position: absolute;
    top: 0;
    right: 0;
    height: 60px;
    z-index: $z-index-dropdown;
  }

  &:active,
  &:focus {
    background-color: darken($color-drawers, 5%);
  }
}

/*================ Drawer loader ================*/
.supports-csstransforms .drawer--is-loading .drawer__cart {
  min-height: 100px;

  &:after {
    content: '';
    display: block;
    width: 24px;
    height: 24px;
    position: absolute;
    left: 50%;
    top: $gutter-site * 2;
    margin-left: -12px;
    border-radius: 50%;
    border: 3px solid $color-drawers-border;
    border-top-color: transparent;
    @include animation(spin 1s infinite linear);
  }
}

/*============================================================================
  Theme-specific drawer styles
==============================================================================*/
.drawer {
  input[type="text"],
  textarea {
    background-color: $color-drawer-inputs;
    color: $color-drawer-input-text;
  }

  a {
    color: $color-drawers-text;

    &:hover,
    &:focus {
      opacity: $opacity-link-hover;
    }
  }
}

.drawer__header {
  // position: absolute;
  // top: 0;
  display: table;
  height: $drawer-header-height;
  width: 100%;
  padding: 0 ($gutter-site / 2);
}

.drawer__inner {
  position: absolute;
  top: $drawer-header-height;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 0 ($gutter-site / 2);
  overflow: auto;
  -webkit-overflow-scrolling: touch;

  @include media-query($large-up) {
    padding-left: $gutter-site;
    padding-right: $gutter-site;
  }

  .drawer--has-fixed-footer & {
    overflow: hidden;
  }

  .drawer--product & {
    top: 0;
  }
}


/*================ Drawer Fixed Cart Footer ================*/
.ajaxcart__inner {
  margin-bottom: $gutter-site;
}

.ajaxcart__inner--has-fixed-footer {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  margin: 0;
  padding: 0 ($gutter-site / 2);
  bottom: $drawer-cart-footer-height; // overwritten with JS
  overflow-x: hidden;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;

  @include media-query($large-up) {
    padding: 0 $gutter-site;
  }
}

.ajaxcart__footer {
  padding-top: $gutter-site;
}

.ajaxcart__footer--fixed {
  position: absolute;
  bottom: 0;
  left: $gutter-site / 2;
  right: $gutter-site / 2;
  min-height: $drawer-cart-footer-height; // overwritten by JS
  padding-bottom: $gutter-site;

  @include media-query($small) {
    padding-bottom: $gutter-site /2;
  }

  @include media-query($large-up) {
    left: $gutter-site;
    right: $gutter-site;
  }
}

/*================ Drawer rows and products ================*/
.ajaxcart__row {
  position: relative;
  max-height: 500px;

  &.is-removed {
    max-height: 0;
    opacity: 0;
    overflow: hidden;
    visibility: hidden;
    transition: all 450ms cubic-bezier(0.57,.06,.05,.95);
    @include backface(hidden);
  }
}

.ajaxcart__product {
  padding-bottom: $gutter-site / 2;
  margin-bottom: $gutter-site / 2;

  @include media-query($medium-up) {
    padding-bottom: $gutter-site;
    margin-bottom: $gutter-site;
  }
}

.ajaxcart__product-image {
  display: block;
  overflow: hidden;

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }
}

.ajaxcart__product-name,
.ajaxcart__product-meta {
  display: block;
}

.ajaxcart__product-meta,
.ajaxcart__price {
  font-size: em(14px);
}

.ajaxcart__product-name-wrapper,
.ajaxcart__product-name {
  margin-bottom: $gutter-site / 4;
}

.ajaxcart__product-name-wrapper {
  padding-left: $gutter-site / 2;
}

.ajaxcart__subtotal {
  margin-bottom: $gutter-site;
}

.ajaxcart__subtotal-title {
  margin-bottom: 0;
  color: $color-drawers-text;
}
/*================ Cart discounts ================*/
.cart-item__original-price,
.cart-item__discount {
  margin-bottom: 0!important;
}

.ajaxcart-item__price-strikethrough {
  float: right;
}

.ajaxcart__savings {
  margin-top: $gutter-heading / 2;
}
/*================ Drawer Nav ================*/

.drawer-nav {
  padding: 0;
  // Negative of .drawer left/right padding for full-width link tap area
  margin: (-$gutter-site / 2) (-$gutter-site / 2) 0;

  @include media-query($large-up) {
    margin-left: -$gutter-site;
    margin-right: -$gutter-site;
  }

  li {
    margin-bottom: 0;
    list-style: none;
  }
}

.drawer-nav__search {
  padding: $gutter-site / 2;
}

.drawer-nav__item {
  display: block;
}

.social-icons--drawer {
  a {
    color: $color-drawers-text;
  }

  @include media-query($small) {
    margin-left: $gutter-site / 2;
  }
}

// Login/logout links can't have a class on them so style all nav <a> elements
.drawer-nav__item a,
.drawer-nav__toggle button {
  color: $color-drawers-text;
  padding: ($gutter-site / 1.5) ($gutter-site / 2);
  text-decoration: none;

  &:active {
    color: darken($color-drawers-text, 15%);
    background-color: darken($color-drawers, 5%);
  }
}

.drawer-nav__item a {
  display: block;
  padding-left: $gutter-site;
  // font-size: em(floor($font-size-header * 0.83)); // 20px
}

.drawer-nav__link {
  margin-bottom: 0;
}

.drawer-nav__toggle-button {
  background: none;
  border: none;
  margin-right: 15px;
}

.drawer-nav__item--secondary {
  a {
    color: $color-drawers-text;
    font-size: $font-size-base;
    line-height: 1.2;
    padding-top: 10px;
    padding-bottom: 10px;
  }
}

.drawer-nav__spacer {
  height: $gutter-site;
}

.drawer-nav__has-sublist {
  display: table;
  width: 100%;

  .drawer-nav__link {
    display: table-cell;
    vertical-align: middle;
    width: 100%;
  }
}

.drawer-nav__toggle {
  display: table-cell;
  vertical-align: middle;
  width: 1%;
}

.drawer-nav__toggle-open {
  display: block;

  .drawer-nav--expanded & {
    display: none;
  }
}

.drawer-nav__toggle-close {
  display: none;
  visibility: hidden;

  .drawer-nav--expanded & {
    display: block;
    visibility: visible;
  }
}

.drawer-nav__sublist {
  margin: 0;
  padding: 0;
  max-height: 0;
  visibility: hidden;
  overflow: hidden;
  transition: all 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);

  .drawer-nav--expanded + & {
    visibility: visible;
    max-height: 1000px;
    margin-bottom: $gutter-site;
    transition: all 600ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  }

  .drawer-nav__link {
    color: $color-drawers-text;
    font-size: $font-size-base;
    padding: ($gutter-site / 2.5) ($gutter-site / 2) ($gutter-site / 2.5) ($gutter-site * 2);
    font-weight: $font-weight-normal;
  }
}

/*============================================================================
  Quantity selectors
  - js-qty replaces input[type="number"] where it's used
  - ajaxcart__qty is used solely in the drawer
==============================================================================*/
.js-qty {
  position: relative;
  margin-bottom: $gutter-site / 2;
  max-width: 100px;
  min-width: 75px;
  overflow: visible;

  input[type="text"] {
    display: block;
    text-align: center;
    width: 100%;
    padding: 2px 20px;
    border-radius: $radius;
  }
}

.js-qty__adjust {
  cursor: pointer;
  position: absolute;
  display: block;
  top: 0;
  bottom: 0;
  border: 0 none;
  padding: 0 8px;
  background: none;
  text-align: center;
  overflow: hidden;
  line-height: 1;
  transition: all 0.2s ease-out;
  @include user-select();
  @include backface(hidden);

  .icon {
    font-size: 8px;
    vertical-align: middle;
  }

  &:hover {
    background-color: darken($color-form, 7%);
  }

  &:active {
    background-color: darken($color-form, 10%);
  }
}

.js-qty__adjust--plus {
  right: 0;
  border-radius: 0 $radius $radius 0;
}

.js-qty__adjust--minus {
  left: 0;
  border-radius: $radius 0 0 $radius;
}

/*================ Quantity Selectors in Ajax Cart ================*/
.ajaxcart__qty {
  @extend .js-qty;
  margin: 0;
  max-width: 75px;
  margin-left: $gutter-site / 2;

  .is-loading & {
    opacity: 0.5;
    transition: none;
  }
}

.ajaxcart__qty-num[type="text"] {
  color: $color-drawers-text;
}

.ajaxcart__qty-adjust {
  @extend .js-qty__adjust;
  color: $color-drawer-input-text;

  &:hover {
    background-color: adaptive-color($color-drawer-inputs, 15%);
  }

  &:active {
    background-color: adaptive-color($color-drawer-inputs, 15%);
  }
}

.ajaxcart__qty--plus {
  @extend .js-qty__adjust--plus;
}

.ajaxcart__qty--minus {
  @extend .js-qty__adjust--minus;
}

/*================ Product details wrapper ================*/
.product__details {
