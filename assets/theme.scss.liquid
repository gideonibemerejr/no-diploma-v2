  font-size: em(floor($font-size-header * 0.58)); // 14px
  color: $color-headings-text;
}

/*================ Theme links and buttons ================*/
.btn {
  position: relative;
  padding: 10px 30px;
  border-radius: $radius;
  @include header-font-stack();
  font-size: $font-size-button; // 14px

  @include media-query($small) {
    font-size: $font-size-button-mobile;
  }

  &.btn--ajax-disabled {
    &:before {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      content: '';
      background-color: $color-btn-primary;
    }

    &:after {
      @include spinner();
      @include animation(spin 0.65s infinite linear);
      left: 50%;
    }
  }

  &[disabled],
  &.btn--disabled {
    opacity: 0.5;
  }
}

.btn--full {
  display: block;
  width: 100%;
}


/*================ Default link styles ================*/
a {
  color: $color-link;
  text-decoration: none;

  &:hover,
  &:focus {
    opacity: $opacity-link-hover;
  }

  &.btn {
    &:hover,
    &:focus {
      opacity: 1;
    }

    &.btn--disabled {
      opacity: 0.5;
    }
  }

}

/*================ Text links ================*/
.link-body-color {
  color: $color-body-text;

  &:hover,
  &:focus {
    opacity: $opacity-link-hover;
  }
}

// make buttons look like links
.btn--link {
