/*================ THEME MIXINS ================*/
/*================ Theme mixins ================*/

@mixin transform($transform) {
  @include prefix(transform, $transform, ms webkit spec);
}

@mixin animation($animation) {
  @include prefix(animation, $animation, webkit moz);
}

@mixin keyframes($name) {
  @-webkit-keyframes #{$name} {
    @content;
  }
  @keyframes #{$name} {
    @content;
  }
}

@mixin header-font-stack {
  font-family: 'Laussane', sans-serif;
  font-weight: $font-weight-header;
  text-rendering: optimizeLegibility;
  {% if settings.type_header_transform %}
    text-transform: uppercase;
  {% endif %}

  {% unless settings.type_header_spacing == blank %}
    letter-spacing: {{ settings.type_header_spacing }};
  {% endunless %}
}

@mixin placeholder-text($color, $opacity) {
  &::-webkit-input-placeholder {
    color: $color;
    opacity: $opacity;
  }

  &::-moz-placeholder {
    color: $color;
    opacity: $opacity;
  }

  &:-ms-input-placeholder {
    color: $color;
    opacity: $opacity;
  }
}

@mixin error-placeholder-text() {
  color: $color-error;
  opacity: 0.6;
}

@mixin gradient($side, $from, $to) {
  background: -ms-linear-gradient($side, rgba($from, 0) 0%, $to 100%);
  background: linear-gradient(to $side, rgba($from, 0) 0%, $to 100%);
}

@mixin spinner($size: 20px, $color: $color-btn-primary-text) {
  content: '';
  display: block;
  width: $size;
  height: $size;
  position: absolute;
  top: 50%;
  margin-left: - $size / 2;
  margin-top: - $size / 2;
  border-radius: 50%;
  border: 3px solid $color;
  border-top-color: transparent;
}

@mixin visually-hidden() {
  position: absolute !important;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px;
  width: 1px;
  margin: -1px;
  padding: 0; border: 0;
}

/*============================================================================
  Flexbox prefix mixins from Bourbon
    https://github.com/thoughtbot/bourbon/blob/master/app/assets/stylesheets/css3/_flex-box.scss
==============================================================================*/
@mixin display-flexbox() {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  width: 100%; // necessary for ie10
}

@mixin flex-wrap($value: nowrap) {
  @include prefix(flex-wrap, $value, webkit moz ms spec);
}

@mixin flex-direction($value) {
  @include prefix(flex-direction, $value, webkit moz ms spec);
}

@mixin align-items($value: stretch) {
  $alt-value: $value;

  @if $value == 'flex-start' {
    $alt-value: start;
  } @else if $value == 'flex-end' {
    $alt-value: end;
  }

  // sass-lint:disable no-misspelled-properties
  -ms-flex-align: $alt-value;
  @include prefix(align-items, $value, webkit moz ms o spec);
}

@mixin flex($value) {
  @include prefix(flex, $value, webkit moz ms spec);
}

@mixin flex-basis($width: auto) {
  // sass-lint:disable no-misspelled-properties
  -ms-flex-preferred-size: $width;
  @include prefix(flex-basis, $width, webkit moz spec);
}

@mixin align-self($align: auto) {
  // sass-lint:disable no-misspelled-properties
  -ms-flex-item-align: $align;
  @include prefix(align-self, $align, webkit spec);
}

@mixin justify-content($justify: flex-start) {
  @include prefix(justify-content, $justify, webkit ms spec);
}

/*================ Keyframe animations ================*/

@include keyframes(fadeIn) {
  0%, 35% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}

@include keyframes(heroContentIn) {
  0%, 35% {
    opacity: 0;
    @include transform(translateY(10px));
  }
  40% {
    @include transform(translateY(10px));
  }
  100% {
    opacity: 1;
    @include transform(translateY(0));
  }
}

@include keyframes(spin) {
  0% {
    @include transform(rotate(0deg));
  }

  100% {
    @include transform(rotate(360deg));
  }
}

/*================ THEME FUNCTIONS ================*/
@function adaptive-color($color, $percentage) {
  @if (lightness( $color ) > 40) {
    @return darken($color, $percentage);
  }
  @else {
    @return lighten($color, $percentage);
  }
}

/*============================================================================
  Strip units off number to have a safe working base, then append units.
  Add unit by multiplying by 1 (ex. 20 * 1px = 20px)
==============================================================================*/
@function add-units($number, $units) {
  $number: $number / ($number * 0 + 1);
  @return $number * 1#{$units};
}


